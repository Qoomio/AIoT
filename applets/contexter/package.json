{
  "name": "contexter-applet",
  "version": "1.0.0",
  "description": "Context-aware database operations and querying applet",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc --noEmit && esbuild src/*.ts --bundle --platform=node --target=es2023 --format=esm --outdir=dist --packages=external",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "test": "tsx watch src/test.ts"
  },
  "keywords": [
    "database",
    "context",
    "query",
    "typescript",
    "applet"
  ],
  "author": "Qoom",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.5.0",
    "@types/ws": "^8.5.12",
    "esbuild": "^0.25.9",
    "tsx": "^4.19.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.1",
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "murmurhash": "^2.0.1",
    "neverthrow": "^8.2.0",
    "tree-sitter": "^0.21.1",
    "tree-sitter-javascript": "^0.23.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.0",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
